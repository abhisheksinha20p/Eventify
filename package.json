{
  "name": "eventify",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "services/*",
    "shared"
  ],
  "scripts": {
    "start": "docker compose up -d",
    "stop": "docker compose down",
    "install:all": "npm install",
    "build:all": "npm run build --workspaces --if-present"
  }
}
